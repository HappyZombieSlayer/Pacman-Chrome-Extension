{
    "manifest_version" : 2,
    "name" : "Pac-Man JS",
    "version" : "3.36",
    "description" : "Pac-Man game popup built using javascript.",
    "icons" : {
        "128" : "images/Pac-Man-Icon.png",
        "48" : "images/Pac-Man-Icon.png",
        "16" : "images/Pac-Man-Icon.png"
    },
    "browser_action" : {
        "default_icon" : "images/Pac-Man-Icon.png",
        "default_popup" : "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
